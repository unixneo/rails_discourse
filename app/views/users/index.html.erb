<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Name</th>
      <th>Seen notification</th>
      <th>Last posted at</th>
      <th>Password hash</th>
      <th>Salt</th>
      <th>Active</th>
      <th>Username lower</th>
      <th>Last seen at</th>
      <th>Admin</th>
      <th>Last emailed at</th>
      <th>Trust level</th>
      <th>Approved</th>
      <th>Approved by</th>
      <th>Approved at</th>
      <th>Previous visit at</th>
      <th>Suspended at</th>
      <th>Suspended till</th>
      <th>Date of birth</th>
      <th>Views</th>
      <th>Flag level</th>
      <th>Ip address</th>
      <th>Moderator</th>
      <th>Title</th>
      <th>Uploaded avatar</th>
      <th>Locale</th>
      <th>Primary group</th>
      <th>Registration ip address</th>
      <th>Staged</th>
      <th>First seen at</th>
      <th>Silenced till</th>
      <th>Group locked trust level</th>
      <th>Manual locked trust level</th>
      <th>Secure identifier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.name %></td>
        <td><%= user.seen_notification_id %></td>
        <td><%= user.last_posted_at %></td>
        <td><%= user.password_hash %></td>
        <td><%= user.salt %></td>
        <td><%= user.active %></td>
        <td><%= user.username_lower %></td>
        <td><%= user.last_seen_at %></td>
        <td><%= user.admin %></td>
        <td><%= user.last_emailed_at %></td>
        <td><%= user.trust_level %></td>
        <td><%= user.approved %></td>
        <td><%= user.approved_by_id %></td>
        <td><%= user.approved_at %></td>
        <td><%= user.previous_visit_at %></td>
        <td><%= user.suspended_at %></td>
        <td><%= user.suspended_till %></td>
        <td><%= user.date_of_birth %></td>
        <td><%= user.views %></td>
        <td><%= user.flag_level %></td>
        <td><%= user.ip_address %></td>
        <td><%= user.moderator %></td>
        <td><%= user.title %></td>
        <td><%= user.uploaded_avatar_id %></td>
        <td><%= user.locale %></td>
        <td><%= user.primary_group_id %></td>
        <td><%= user.registration_ip_address %></td>
        <td><%= user.staged %></td>
        <td><%= user.first_seen_at %></td>
        <td><%= user.silenced_till %></td>
        <td><%= user.group_locked_trust_level %></td>
        <td><%= user.manual_locked_trust_level %></td>
        <td><%= user.secure_identifier %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
